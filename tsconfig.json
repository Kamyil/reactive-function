{
  "compilerOptions": {
    "baseUrl": "./",
    "module": "es6",
    "target": "es5",
    "sourceRoot": "src",
    "sourceMap": true,
    "moduleResolution": "Classic",
    "lib": ["dom", "ES2020"],
    "strictNullChecks": false,
    "strictPropertyInitialization": false,
    "esModuleInterop": false,
    "allowSyntheticDefaultImports": false
  },
  "typeRoots": ["node_modules/@types", "types.d.ts"]
}